<Page x:Class="QuanLyChung365TruocDangNhap.ChamCong365.Views.PopUp.PopUp_SettingAccount.Add_Image"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:QuanLyChung365TruocDangNhap.ChamCong365.Views.PopUp.PopUp_SettingAccount"
      mc:Ignorable="d" 
      d:DesignHeight="96" d:DesignWidth="136"
      Title="Add_Image">

    <Grid>
        <Border CornerRadius="5" BorderThickness="0" Background="{DynamicResource #4C5BD4}" Opacity="0.8">
            <Path Data="{DynamicResource iconCamera}" Fill="{DynamicResource #FFFFFF}" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        </Border>
    </Grid>
</Page>
